<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="TestHashSetPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entidades.Socios</class>
    <class>entidades.Penalizaciones</class>
    <class>entidades.Eventos</class>
    <class>entidades.Estanterias</class>
    <class>entidades.Elementos</class>
    <class>entidades.Prestamos</class>
    <class>entidades.Estados</class>
    <class>entidades.Libros</class>
    <class>entidades.Visionados</class>
    <class>entidades.Dvds</class>
    <class>entidades.Concursos</class>
    <class>entidades.Proveedores</class>
    <class>entidades.Cursos</class>
    <class>entidades.Lecturas</class>
    <class>entidades.Generos</class>
    <class>entidades.Lotes</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/biblioteca?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
    </properties>
  </persistence-unit>
</persistence>
